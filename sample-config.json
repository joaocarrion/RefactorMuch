{
  "lastLeft": "",
  "lastRight": "",
  "lastItems": [],
  "fileFilters": {
    "extensions": ["*.cs", "*.unity", "*.prefab"]
  },
  "tools": [
    {
      "type": "Diff",
      "path": "C:\\Program Files (x86)\\WinMerge\\WinMergeU.exe",
      "parameters": "/s"
    },
    {
      "type": "View",
      "path": "C:\\Program Files\\Microsoft VS Code\\Code.exe",
      "parameters": "-r"
    }
  ],
  "rules": [
    {
      "name": "FileCompare",
      "description": "Rules for comparing an entire file",
      "rules": [
        {
          "name": "Block Comments",
          "description": "Remove block comments",
          "expression": "(\\/\\*)(.|\\n)*?(\\*\\/)"
        },
        {
          "name": "Line Comments",
          "description": "Remove line comments",
          "expression": " *\\/\\/.*"
        },
        {
          "name": "Syntax Breakers",
          "description": "Remove syntax breakers",
          "expression": " {2}|\\{|\\}|(\\r\\n)"
        }
      ]
    },
    {
      "name": "LineCompare",
      "description": "Rules for comparing an entire file",
      "rules": [
        {
          "name": "Block Comments",
          "description": "Remove block comments",
          "expression": "(\\/\\*)(.|\\n)*?(\\*\\/)"
        },
        {
          "name": "Line Comments",
          "description": "Remove line comments",
          "expression": " *\\/\\/.*"
        },
        {
          "name": "Syntax Breakers",
          "description": "Remove syntax breakers (leave \\n)",
          "expression": " {2}|\\{|\\}|(\\r)"
        }
      ]
    },
    {
      "name": "Normalize",
      "description": "Normalize files for comparison",
      "rules": [
        {
          "name": "Normalize",
          "description": "Normalize files for comparison",
          "expression": "\\r"
        }
      ]
    }
  ]
}
